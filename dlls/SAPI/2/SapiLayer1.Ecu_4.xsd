                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="Qualifier" form="unqualified" type="xs:string" />
                        <xs:attribute name="Name" form="unqualified" type="xs:string" />
                        <xs:attribute name="GroupQualifier" form="unqualified" type="xs:string" />
                        <xs:attribute name="GroupName" form="unqualified" type="xs:string" />
                        <xs:attribute name="HideComponents" form="unqualified" type="xs:string" />
                        <xs:attribute name="Description" form="unqualified" type="xs:string" />
