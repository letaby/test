                </xs:complexType>
              </xs:element>
              <xs:element name="ComParameters" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ComParameter" nillable="true" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute name="Name" form="unqualified" type="xs:string" />
                          </xs:extension>
