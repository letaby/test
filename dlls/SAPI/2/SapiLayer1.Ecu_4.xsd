                  <xs:sequence>
                    <xs:element name="Property" nillable="true" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:simpleContent>
                          <xs:extension base="xs:string">
                            <xs:attribute name="Name" form="unqualified" type="xs:string" />
