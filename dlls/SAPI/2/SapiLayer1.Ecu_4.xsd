                              <xs:attribute name="Index" form="unqualified" type="xs:integer" />
                              <xs:attribute name="Value" form="unqualified" type="xs:string" />
                            </xs:complexType>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="Name" form="unqualified" type="xs:string" />
                        <xs:attribute name="Qualifier" form="unqualified" type="xs:string" />
                        <xs:attribute name="ReferenceQualifier" form="unqualified" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
