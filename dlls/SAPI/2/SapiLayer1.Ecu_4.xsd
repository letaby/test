            <xs:attribute name="AutoConnect" form="unqualified" type="xs:boolean" />
          </xs:complexType>
        </xs:element>
      </xs:sequence>  
      <xs:attribute name="Time" form="unqualified" type="xs:string" />
    </xs:complexType>
  </xs:element>
</xs:schema>