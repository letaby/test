                        <xs:attribute name="FormatString" form="unqualified" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="Instruments" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Instrument" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:attribute name="Qualifier" form="unqualified" type="xs:string" />
                        <xs:attribute name="CacheTime" form="unqualified" type="xs:string" />
                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="ServicesAsInstruments" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="ServiceAsInstrument" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="ReferenceInputValue" minOccurs="0" maxOccurs="unbounded">
