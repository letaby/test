<?xml version="1.0" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="SapiFile">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="CBFVersions" minOccurs="0" maxOccurs="unbounded">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="CBF" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                  <xs:attribute name="ecu" form="unqualified" type="xs:string" />
                  <xs:attribute name="version" form="unqualified" type="xs:string" />
                </xs:complexType>
              </xs:element>
            </xs:sequence>
          </xs:complexType>
        </xs:element>
        <xs:element name="Ecu" maxOccurs="1">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Properties" minOccurs="0">
