                      </xs:complexType>
                    </xs:element>
                  </xs:sequence>
                </xs:complexType>
              </xs:element>
              <xs:element name="EcuInfos" minOccurs="0">
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="EcuInfo" maxOccurs="unbounded">
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element name="Reference" minOccurs="0" maxOccurs="unbounded">
